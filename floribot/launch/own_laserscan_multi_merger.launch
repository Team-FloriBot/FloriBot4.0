<!-- DESCRITPION -->

<launch>
	<node pkg="ira_laser_tools" name="laserscan_multi_merger" type="laserscan_multi_merger" output="screen">
	<param name="destination_frame" value="laserFront"/>
	<param name="cloud_destination_topic" value="/merged_cloud"/>
	<param name="scan_destination_topic" value="/sensors/scanMulti"/>
	<param name="laserscan_topics" value ="/sensors/scanFront /sensors/scanRear" /> <!-- LIST OF THE LASER SCAN TOPICS TO SUBSCRIBE -->
	<param name="angle_min" value="-3.14159265359"/>
	<param name="angle_max" value="3.14159265359"/>
	<param name="angle_increment" value="0.005817763973027468"/>
	<param name="scan_time" value="0.05"/>
	<param name="range_min" value="0.05"/>
	<param name="range_max" value="25.0"/>
	</node>
</launch>
